<table border="0">
  <thead>
    <tr>
      <th>标题</th>
      <th>时间</th>
    </tr>
  </thead>
  <tbody>
    <% @posts.each do |post| %>
      <tr>
        <td><%= link_to post.name, post.url %></td>
        <td><%= post.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= paginate @posts %>
